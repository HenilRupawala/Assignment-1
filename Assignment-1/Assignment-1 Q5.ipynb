   "source": [
    "a = int(input('Enter the Number a:' ))\n",
    "b = int(input('Enter the Number b:' ))\n",
    "\n",
    "count = 0\n",
    "while a % b == 0:\n",
    "    count += 1  \n",
    "    a = a / b\n",
    "\n",
    "if count > 0:\n",
    "    print(f\"{a} can be divided by {b} {count} times\")\n",
    "else:\n",
    "    print(f\"{a} is not divisible by {b}\")"
   ]
  }
